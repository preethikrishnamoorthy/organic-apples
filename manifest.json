{
    "manifest_version": 2,
    "name": "Organic Apples",
    "description": "show url.",
    "version": "0.2",
    "background": {
        "scripts": ["event.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "images/get_started16.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ]
}
